datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-musl"]
}

// Modelo de Clientes
model Client {
  id        String  @id @default(uuid())
  name      String
  email     String  @unique
  isDeleted Boolean @default(false) @map("is_deleted")

}

